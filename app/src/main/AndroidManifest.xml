<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="wang.raye.myview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MoreTextViewActivity"
            android:label="MoreTextViewActivity" />
        <activity
            android:name=".CollapsedActivity"
            android:label="CollapsedActivity" />
        <activity
            android:name=".AnimActivity"
            android:label="AnimActivity" />
        <activity
            android:name=".BinderActivity"
            android:label="BinderActivity" />

        <service
            android:name=".services.BinderService"
            android:enabled="true"
            android:exported="true"></service>
    </application>

</manifest>